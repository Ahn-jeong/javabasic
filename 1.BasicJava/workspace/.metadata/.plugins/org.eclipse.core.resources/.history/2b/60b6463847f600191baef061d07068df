</script>
<script>
	function change(n) {
		if (n == 1) {
			document.getElementById("middle5title1").style.color = "red";
			document.getElementById("middle5title2").style.color = "black";
			document.getElementById("middle5ul1").style.display = "inline-block";
			document.getElementById("middle5ul2").style.display = "none";

		} else if (n == 2) {
			document.getElementById("middle5title2").style.color = "red";
			document.getElementById("middle5title1").style.color = "black";
			document.getElementById("middle5ul2").style.display = "inline-block";
			document.getElementById("middle5ul1").style.display = "none";
		}
	}
</script>
